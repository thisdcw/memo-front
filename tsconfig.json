{
  "compilerOptions": {
    "module": "commonjs", // 或 "esnext"，取决于你的需求
    "target": "esnext",
    "noEmit": false, // 确保这个选项设置为 false
    "esModuleInterop": true,
    "skipLibCheck": true,
    "composite": true,
    "baseUrl": ".",
    "outDir": "./dist", // 设置输出目录
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  // "files": [],
  // "references": [
  //   {
  //     "path": "./tsconfig.app.json"
  //   },
  //   {
  //     "path": "./tsconfig.node.json"
  //   }
  // ],
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "exclude": ["node_modules"]
}
